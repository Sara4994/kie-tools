{
  "name": "BPMN and DMN Editors :: GitHub Extension",
  "version": "0.2.5",
  "manifest_version": 2,
  "description": "KIE Group :: GitHub Extension",
  "content_scripts": [
    {
      "run_at": "document_idle",
      "js": ["content_scripts/github.js"],
      "matches": ["https://github.com/*"],
      "all_frames": true
    },
    {
      "run_at": "document_idle",
      "js": ["content_scripts/online-editor.js"],
      "matches": ["*://paulovmr.github.io/*", "*://localhost/*"],
      "all_frames": true
    }
  ],
  "background": { "scripts": ["background.js"], "persistent": true },
  "web_accessible_resources": ["resources/*"],
  "permissions": [
    "webRequestBlocking",
    "webRequest",
    "https://*/*",
    "http://*/*",
    "tabs",
    "declarativeContent",
    "background"
  ],
  "content_security_policy": "script-src 'self' https://raw.githubusercontent.com; object-src 'self'",
  "externally_connectable": {
    "matches": ["*://*.github.com/*", "*://paulovmr.github.io/*", "*://localhost/*"]
  },
  "key": ""
}
